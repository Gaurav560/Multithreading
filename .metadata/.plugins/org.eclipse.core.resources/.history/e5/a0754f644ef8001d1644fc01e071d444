package com.ineuron.ai;

 class My1 extends Thread{

	@Override
	public void run() {
//task for thread
		for(int i=1;i<=10;i++) {
			System.out.println(i);
			Thread.sleep("20000L");
		}
	}

	
	
}

public class MyThread2{
public static void main(String[] args) {
	My1 t=new My1();
	
	t.start();
}	
}
